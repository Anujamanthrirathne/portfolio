{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"  
      }
    }
  ],
  "routes": [
    {
      "src": "/desktop_pc/(.*)",
      "dest": "/desktop_pc/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
