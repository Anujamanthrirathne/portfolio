{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/subfolder"  
      }
    }
  ],
  "routes": [
    {
      "src": "/subfolder/assets/(.*)",
      "dest": "/subfolder/assets/$1"
    },
    {
      "src": "/subfolder/(.*)",
      "dest": "/subfolder/index.html"
    }
  ]
}
